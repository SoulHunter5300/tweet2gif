{
  "compilerOptions": {
    "target": "es5",
    "outDir": "dist",
    "lib": [
        "dom",
        "es2017"
    ],
    "moduleResolution": "node",
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "strictNullChecks": false,
    "noUnusedLocals": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
